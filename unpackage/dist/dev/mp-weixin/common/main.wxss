














/*每个页面公共css */
page{
	-one-title-color:#303133; /* - 一级信息 标题 */
	--ordinary-text-color:#606266; /*普通文字*/
	--secondary-text-color:#909399; /*次要信息*/
	--describe-text-color:#bfc2cc;/*描述信息*/
}
page{
	padding: 0 20rpx;
	background-color: f9f9f9;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
view{
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
view text{
	font-family:PingFang SC;
	font-weight:500;
}
.content{
	margin: 0 auto;
}
.layout{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
/* 水平垂直居中 */
.layout-xy-center{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
/* 次要信息 */
.secondary-text{
	color: var(--secondary-text-color);
	font-size: 24rpx;
}
.one-title{
	color: var(--one-title-color);
	font-size: 32rpx;
	font-weight:bold;
}
.dele {
		text-decoration: line-through;
}
.complete-btn{
	width:320rpx;
    height:86rpx;
	line-height: 86rpx;
	text-align: center;
	background-color: #FFCD00;
	border-radius:43rpx;
	color: #222222;
	font-size: 36rpx;
}
.reset-btn{
	width:320rpx;
    height:86rpx;
	line-height: 86rpx;
	text-align: center;
	background-color: #F9F9F9;
	border-radius:43rpx;
	color: #222222;
	font-size: 36rpx;
}
image{
	width: 100%;
	height: 100%;
}


